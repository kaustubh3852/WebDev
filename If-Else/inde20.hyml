<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Javacript</title>
</head>
<body>
    <h2>Javacript For Loop</h2>
    <p id="demo"></p>

    <script>
        const cars = ["BMW", "Volvo", "Saab", "Ford"];
        let i = 0;
        let len = cars.length;
        let text = "";

        for(; i < len; i++){
            text += cars[i] + "<br>";
        }
        document.getElementById('demo').innerHTML = text;
        document.getElementById('demo').innerHTML = text;
        document.getElementById('demo').innerHTML = text;
    </script>
</body>
</html>